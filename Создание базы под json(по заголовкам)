CREATE TABLE ClosureMPS (id text not null PRIMARY KEY,type_id text,state_id text,ts_to timestamp,ts_from timestamp,speed_reduced int,descr varchar(255),lanes_available int, capacity_reduced int,traffic_relevant int,confirmed boolean,public varchar(50),commission_number varchar (50), requester varchar (50), disabled boolean,address varchar (250), lanes_closed int,ext_id bigint,created_by text,created_at timestamp,updated_by text,updated_at timestamp,datamos_id int,work_type_id text,customer varchar(250),telephone_number varchar(100),okrug_id int,documentation_file varchar(250),currentness_type_id text,full_overlap boolean,event_work_type text,name varchar(150),event_type_id text,event_type text,currentness_type text,okrug text,event_state varchar(50),event_documents varchar(150)
)
